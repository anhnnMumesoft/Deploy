rt loading\r\n        let res = await getAllBlog({\r\n            subjectId:'',\r\n            limit: '',\r\n            offset:'',\r\n            keyword:''\r\n        })\r\n        setIsLoading(false); // End loading after all fetches\r\n        if(res && res.errCode == 0){\r\n            res.data.forEach(element => {\r\n                element.image = \"\"\r\n            })\r\n            await CommonUtils.exportExcel(res.data,\"Danh sách bài viết\",\"ListBlog\")\r\n        }\r\n       \r\n    }\r\n    return (\r\n        <div className=\"container-fluid px-4\">\r\n         {isLoading && <LoadingSpinner />}\r\n            <h1 className=\"mt-4\">Quản lý bài đăng</h1>\r\n\r\n\r\n            <div className=\"card mb-4\">\r\n                <div className=\"card-header\">\r\n                    <i className=\"fas fa-table me-1\" />\r\n                    Danh sách bài đăng\r\n                </div>\r\n                <div className=\"card-body\">\r\n              \r\n                    <div className='row'>\r\n                    <div  className='col-4'>\r\n                    <FormSearch title={\"tiêu đề\"}  handleOnchange={handleOnchangeSearch} handleSearch={handleSearchBlog} />\r\n                    </div>\r\n                    <div className='col-8'>\r\n                    <button  style={{float:'right'}} onClick={() => handleOnClickExport()} className=\"btn btn-success\" >Xuất excel <i class=\"fa-solid fa-file-excel\"></i></button>\r\n                    </div>\r\n                    </div>\r\n                    <div className=\"table-responsive\">\r\n                        <table className=\"table table-bordered\" style={{ border: '1' }} width=\"100%\" cellspacing=\"0\">\r\n                            <thead>\r\n                                <tr>\r\n                                    <th>STT</th>\r\n                                    <th>Tên bài đăng</th>\r\n                                    <th>Chủ đề</th>\r\n                                    <th>