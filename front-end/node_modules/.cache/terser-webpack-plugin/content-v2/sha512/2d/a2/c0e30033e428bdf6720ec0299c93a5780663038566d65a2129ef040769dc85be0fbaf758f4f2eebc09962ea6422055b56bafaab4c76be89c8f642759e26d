monUtils from '../../../utils/CommonUtils';\r\nimport {\r\n    BrowserRouter as Router,\r\n    Switch,\r\n    Route,\r\n    Link,\r\n    Redirect\r\n} from \"react-router-dom\";\r\nimport ReactPaginate from 'react-paginate';\r\nimport FormSearch from '../../../component/Search/FormSearch';\r\nimport LoadingSpinner from '../../../component/Spinner/LoadingSpinner';\r\nconst ManageCategory = () => {\r\n    const [isLoading, setIsLoading] = useState(false);\r\n    const [dataCategory, setdataCategory] = useState([])\r\n    const [count, setCount] = useState('')\r\n    const [numberPage, setnumberPage] = useState('')\r\n    const [keyword, setkeyword] = useState('')\r\n    useEffect(() => {\r\n       \r\n            fetchData(keyword);\r\n        \r\n\r\n    }, [])\r\n    let fetchData = async (keyword) => {\r\n        setIsLoading(true); // Start loading\r\n        let arrData = await getListAllCodeService({\r\n\r\n            type: 'CATEGORY',\r\n            limit: PAGINATION.pagerow,\r\n            offset: 0,\r\n            keyword:keyword\r\n\r\n        })\r\n        setIsLoading(false); // End loading after all fetches\r\n        if (arrData && arrData.errCode === 0) {\r\n            setdataCategory(arrData.data)\r\n            setCount(Math.ceil(arrData.count / PAGINATION.pagerow))\r\n        }\r\n    }\r\n    let handleDeleteCategory = async (event, id) => {\r\n        event.preventDefault();\r\n        setIsLoading(true); // Start loading\r\n        let res = await DeleteAllcodeService(id)\r\n        setIsLoading(false); // End loading after all fetches\r\n        if (res && res.errCode === 0) {\r\n            toast.success(\"Xóa danh mục thành công\")\r\n            let arrData = await getListAllCodeService({\r\n\r\n                type: 'CATEGORY',\r\n                limit: PAGINATION.pagerow,\r\n                offset: numberPage * PAGINATION.pagerow,\r\n                keyword:keyword\r\n\r\n            })\r\n            if (arrData && arrData.errCode === 0) 